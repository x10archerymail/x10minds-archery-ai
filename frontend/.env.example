# ========================================
# X10Minds AI - Environment Variables Template
# ========================================
# Copy this file to .env and fill in your actual API keys
# ========================================

# Google Gemini API Key (Main AI Service)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key (Alternative AI Service)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Firebase Configuration - Main App (AI)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# reCAPTCHA Site Key
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Firebase Configuration - Shop
VITE_FIREBASE_SHOP_API_KEY=your_shop_firebase_api_key_here
VITE_FIREBASE_SHOP_AUTH_DOMAIN=your-shop-project.firebaseapp.com
VITE_FIREBASE_SHOP_PROJECT_ID=your-shop-project-id
VITE_FIREBASE_SHOP_STORAGE_BUCKET=your-shop-project.firebasestorage.app
VITE_FIREBASE_SHOP_MESSAGING_SENDER_ID=your_shop_sender_id
VITE_FIREBASE_SHOP_APP_ID=your_shop_app_id
VITE_FIREBASE_SHOP_MEASUREMENT_ID=your_shop_measurement_id

# Payment Integration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key_here

# App Settings
VITE_APP_NAME=X10Minds Archery AI Coach
VITE_BASE_URL=http://localhost:5173
